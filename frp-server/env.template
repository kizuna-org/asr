# frp-server/env.template - FRP Server Environment Variables Template
# Copy this to .env and update values for production

# FRP認証設定
FRP_TOKEN=jenkins_frp_secret_token

# Webダッシュボード認証
FRP_DASHBOARD_USER=admin
FRP_DASHBOARD_PASSWORD=frp_admin_password

# ポート設定
FRP_BIND_PORT=7000
FRP_DASHBOARD_PORT=8000
FRP_HTTP_PORT=80
FRP_HTTPS_PORT=443

# ドメイン設定
FRP_SUBDOMAIN_HOST=shiron.dev
